<launch>
  <node
      pkg="robot_voice"
      type="string_to_voice"
      name="string_to_voice"
      output="screen"
  />


  <node
      pkg="robot_voice"
      type="robot_controller"
      name="robot_controller"
      output="screen"
  />

  <node
      pkg="robot_voice"
      type="voice_to_string"
      name="voice_to_string"
      launch-prefix="bash -c 'sleep 5; $0 $@'"
      output="screen"
  />

</launch>
